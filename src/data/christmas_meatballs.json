    { "groupId": 1, "amount": 2, "unit": "pinches", "unitSymbol": "krm", "ingredientName": "svartpeppar" },
    { "groupId": 3, "amount": 1, "unit": "piece", "unitSymbol": "st", "ingredientName": "gul lök" },
